{% extends 'base.html.twig' %}
{% block title%}
    {{ title }}
{% endblock %}
{% block body %}
    <h1>{{ 'Stations' }}</h1>
    <p>{{  message }}</p>
<div>
    {{ form(form) }}
    {% for station in stations %}
        <li>
            {{ station.ID }}  {{ station.StationType }}  {{ station.Power }}
        </li>
    {% endfor %}
</div>
{% endblock %}